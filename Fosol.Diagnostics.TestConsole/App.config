<?xml version="1.0"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  
  <system.diagnostics>
    <sources>
      <source name="Fosol.Diagnostics.TestConsole" switchValue="All">
        <listeners>
          <add name="console"/>
        </listeners>
      </source>
    </sources>
    
    <sharedListeners>
      <add name="console" type="Fosol.Log.Listeners.ConsoleListener, Fosol.Log" traceOutputOptions="DateTime, Timestamp"/>
    </sharedListeners>
  </system.diagnostics>
</configuration>
